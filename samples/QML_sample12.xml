<?xml version="1.0"?>
<!-- test 12 : a simple matrix quantity with one set of axes and tagged values -->
<matrixQuantity
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns="http://archive.astro.umd.edu/ont/Quantity.owl"
      xsi:schemaLocation="http://archive.astro.umd.edu/ont/Quantity.owl QML_10.xsd"
      size="3"
>
   <axisFrame>
       <listQuantity size="3"><integer width="1"/><values><![CDATA[0 1 2]]></values></listQuantity>
   </axisFrame>
   <string width="4"/>
   <values><value>some</value><value>data</value><value>here</value></values> 
</matrixQuantity>
