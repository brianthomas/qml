<?xml version="1.0" standalone="no"?>
<!-- test 20: test vector data type w/ 2 components where values are "tagged" -->
<matrixQuantity
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.data-model.net/Quantity QML_05.xsd"
      xmlns="http://www.data-model.net/Quantity"
      size="6"
>
   <vector>
      <component name="RA">
         <units>deg</units>
         <float width="6" precision="2"/>
      </component>
      <component name="Dec">
         <units>deg</units>
         <float width="6" precision="2"/>
      </component>
   </vector>
   <!-- the values are a series of component (coordinate) pairs -->
   <values>
      <value>10.12 -12.34</value>
      <value>20.34 -13.87</value>
      <value>24.76 +5.67</value>
      <value>6.80 -80.73</value>
      <value>210.12 12.34</value>
     <value>20.34 -13.87</value>
   </values>
</matrixQuantity>
