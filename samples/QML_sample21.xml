<?xml version="1.0" standalone="no"?>
<!-- test 21: test vector data type, w/ 2-components and data are untagged-->
<matrixQuantity
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://archive.astro.umd.edu/ont/Quantity.owl QML_10.xsd"
      xmlns="http://archive.astro.umd.edu/ont/Quantity.owl"
      size="6"
>
   <vector>
      <component name="RA">
         <units>deg</units>
         <float width="6" precision="2"/>
      </component>
      <component name="Dec">
         <units>deg</units>
         <float width="6" precision="2"/>
      </component>
   </vector>
   <!-- the values are a series of component (coordinate) pairs -->
   <values> 10.12  -12.34
            20.34  -13.87
            24.76   +5.67
             6.80  -80.73
           210.12   12.34
            20.34  -13.87
</values>
</matrixQuantity>
