<?xml version="1.0"?>
<!-- test 14 : a simple matrix quantity with several sets of axes -->
<matrixQuantity
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns="http://www.data-model.net/Quantity"
      xsi:schemaLocation="http://www.data-model.net/Quantity QML_05.xsd"
      size="9"
>
   <axisFrame>
       <listQuantity size="3"><integer width="1"/><values><![CDATA[0 1 2]]></values></listQuantity>
       <listQuantity size="3"><integer width="1"/><values><![CDATA[0 1 2]]></values></listQuantity>
   </axisFrame>
   <axisFrame>
       <listQuantity size="9"><string width="1"/><values>A B C D E F G H I</values></listQuantity>
   </axisFrame>
   <string width="4"/>
   <values>0 1 2 3 4 5 6 7 8</values>
</matrixQuantity>
