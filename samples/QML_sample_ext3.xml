<?xml version="1.0"?>
<!-- test 3: The use/creation of ellipse quantity, having members
     which are "semi-major" and "semi-minor" (also quantities).
     Its imported in to MyDocument schema from Ellipse.xsd schema.
  -->
<doc:mydocument
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:doc="http://noplace.org/MyDocument"
      xmlns:q="http://www.data-model.net/Quantity"
      xmlns="http://noplace.org/Ellipse"
      xsi:schemaLocation="http://noplace.org/MyDocument MyDocument.xsd"
>
   <ellipse uri="urn:ellipse-region">
       <center uri="urn:center">
           <q:vector>
              <q:component uri="urn:x_position-coordinate" name="x pos"><q:units>deg</q:units></q:component>
              <q:component uri="urn:y_position-coordinate" name="y pos"><q:units>deg</q:units></q:component>
           </q:vector>
           <q:value>10 10</q:value>
       </center>
       <radius uri="urn:radius-of-ellipse">
          <q:units>deg</q:units>
          <q:integer width="2"/>
          <q:value>10</q:value>
       </radius>
       <minorRadius uri="urn:minor-radius">
          <q:units>deg</q:units>
          <q:integer width="2"/>
          <q:value>5</q:value>
       </minorRadius>
       <positionAngle uri="urn:position-angle">
          <q:units>rad</q:units>
          <q:float width="3" precision="1"/>
          <q:value>1.2</q:value>
       </positionAngle>
    </ellipse>
</doc:mydocument>
