<?xml version="1.0"?>
<!-- Builder quantity test, build from RDF model where all of the components
     are present. THis is AQ with float value.
  -->
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:j.0="http://test.org/testThings.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
    xmlns:ui="http://archive.astro.umd.edu/ont/unit_instances.owl#" 
    xmlns:u="http://archive.astro.umd.edu/ont/unit_ontology.owl#" 
    xmlns:q="http://archive.astro.umd.edu/ont/Quantity.owl#" 
    > 
    
  <rdf:Description rdf:about="http://test.org/testOps.owl#quantity1">
  <!--
    <q:name>Time Passed</q:name>
    <q:description>the amount of time passed</q:description>
    <q:hasUnits rdf:resource="http://archive.astro.umd.edu/ont/Quantity.owl#second"/>
    <q:hasUnits><q:second/></q:hasUnits>
    -->
    <q:hasUnits rdf:resource="http://test.org/testOps.owl#unit1"/>
    <q:hasDataType rdf:resource="http://test.org/testOps.owl#datatype1"/>
    <q:value>1.0</q:value>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://archive.astro.umd.edu/ont/Quantity.owl#Quantity"/>
    <rdf:type rdf:resource="http://test.org/testThings.owl#FloatQ"/>
    <rdf:type rdf:resource="http://test.org/testThings.owl#FloatQ3"/>
    <owl:sameAs rdf:resource="http://test.org/testOps.owl#quantity1"/>
  </rdf:Description>
  
  <!--
  <q:second rdf:about="http://test.org/testOps.owl#unit1"/> 
  -->
  <q:Units rdf:about="http://test.org/testOps.owl#unit1"> 
     <q:symbol>s</q:symbol>
  </q:Units>
  
  <rdf:Description rdf:about="http://test.org/testOps.owl#datatype1">
    <q:width>3</q:width>
    <q:precision>1</q:precision>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://archive.astro.umd.edu/ont/Quantity.owl#NumericalDataType"/>
    <rdf:type rdf:resource="http://archive.astro.umd.edu/ont/Quantity.owl#FloatDataType"/>
  </rdf:Description>
  
</rdf:RDF>
