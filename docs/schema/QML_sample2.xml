<?xml version="1.0" standalone="no"?>
<!-- test 2 : explicit namespace, integer datatype, and units of cm -->
<q:atomicQuantity 
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
      xmlns:q="http://www.data-model.net/Quantity" 
      xsi:schemaLocation="http://www.data-model.net/Quantity QML_32.xsd"
>
   <q:units>cm</q:units>
   <q:integer width="2" signed="no"/>
   <q:value>10</q:value>
</q:atomicQuantity>
