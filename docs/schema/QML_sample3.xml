<?xml version="1.0"?>
<!-- test 3: meta data on node -->
<q:atomicQuantity
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:so="http://www.data-model.net/SemanticObject"
      xmlns:q="http://www.data-model.net/Quantity"
      xsi:schemaLocation="http://www.data-model.net/Quantity Quantity.xsd"
>
   <!-- child Q is meta-data (in relationship) with the 'parent' Q -->
   <q:relationship URI="urn:qml:hasMember">
       <q:atomicQuantity name="a value"><q:value>data</q:value></q:atomicQuantity>
   </q:relationship>
   <q:relationship URI="urn:qml:hasMember">
       <so:semanticObject URI="urn:qml:dude"/>
   </q:relationship>
   <q:integer width="4"/>
   <q:value>1000</q:value>
</q:atomicQuantity>
